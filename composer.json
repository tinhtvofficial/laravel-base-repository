{
    "name": "luuka/laravel-base-repository",
    "description": "A simple package to create a make:repository command for Laravel",
    "require": {
        "php": ">=5.4.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Luuka\\LaravelBaseRepository\\RepositoryServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Luuka\\LaravelBaseRepository\\": "src/"
        },
        "classmap": ["src/"]
    },
    "authors": [
        {
            "name": "Trương Tỉnh",
            "email": "tinhtv1910@gmail.com"
        }
    ]
}
